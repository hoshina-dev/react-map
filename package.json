{
  "name": "react-map-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build --continue=dependencies-successful",
    "dev": "turbo run dev",
    "lint": "turbo run lint --continue && prettier --check \"**/*.md\"",
    "lint-strict": "turbo run lint-strict --continue && prettier --check \"**/*.md\"",
    "format": "turbo run format && prettier --write \"**/*.md\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.8.0",
    "turbo": "^2.7.5",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": "^24.11"
  }
}
